{
  "name": "@0xflair/react-icons",
  "version": "0.25.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xflair/typescript-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "generate-barrels": "barrelsby -d ./build",
    "svgr": "svgr --title-prop --icon --no-dimensions --typescript -d ./build/ assets",
    "build": "npm run clean && npm run svgr && npm run generate-barrels && rollup -c"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@svgr/cli": "^6.2.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "barrelsby": "^2.3.4",
    "ethers": "^5.6.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^4.6.2",
    "usehooks-ts": "^2.4.2",
    "webpack": "^5.70.0"
  }
}
